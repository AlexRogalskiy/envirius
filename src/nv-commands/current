nv_cmd_default() {
    # deactivate previusly activated environment
    if [ "$NV_USED_ENV" != "" ]; then
        echo $NV_USED_ENV
    fi
}

nv_cmd_desc() {
    echo "Output current environment's name"
}

nv_cmd_help() {
    show_usage
    show_desc "$(nv_cmd_desc)"
}

# /* vim: set filetype=sh : */
