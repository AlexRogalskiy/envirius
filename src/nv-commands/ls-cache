nv_cmd_default() {
    for entry in $(find $(nv_get_arc_path) -mindepth 1 -maxdepth 1)
    do
        nv_get_filename $entry
    done
}

nv_cmd_desc() {
    echo "List locally cached verions (no need to download & build)"
}

nv_cmd_help() {
    nv_show_usage
    nv_show_desc "$(nv_cmd_desc)"
}

# /* vim: set filetype=sh : */
