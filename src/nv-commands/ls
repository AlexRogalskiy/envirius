mem_cmd_default() {
    echo "Available environment(s):"
    for env in $(find $(get_env_full_path) -mindepth 1 -maxdepth 1 -type d)
    do
        # display dirname only
        filename $env
    done
}

mem_cmd_desc() {
    echo "List environments"
}

mem_cmd_help() {
    show_usage
    show_desc "$(mem_cmd_desc)"
}

