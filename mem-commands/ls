mem_cmd_default() {
    echo "Available environment(s):"
    for env in $(find $MEM_ENVS_DIR -mindepth 1 -maxdepth 1 -type d)
    do
        # display dirname only
        filename $env
    done
}

mem_cmd_desc() {
    echo "list environments"
}

mem_cmd_help() {
    show_usage "ls"
    show_desc "$(mem_cmd_desc)"
}

