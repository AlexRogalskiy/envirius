mem_cmd_default() {
    env_name=""

    # try to find env name
    for par in "$@"
    do
        env_name_full=$MEM_ENVS_DIR/$par
        if [ -d "$env_name_full" ]; then
            rm -rf $env_name_full
        fi
    done
}

mem_cmd_desc() {
    echo "delete environment"
}

mem_cmd_help() {
    show_usage "rm env [env, env, ...]"
    show_desc "$(mem_cmd_desc)"
}

# /* vim: set filetype=sh : */
