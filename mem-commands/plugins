mem_cmd_default() {
    for plugin in $(find $MEM_PLUGINS_DIR \
                    -type f \
                    -not -iname "common" \
                    -not -iname ".*" | sort)
    do
        filename $plugin
    done
}

mem_cmd_desc() {
    echo "list available plugins"
}

mem_cmd_help() {
    show_usage "plugins"
    show_desc "$(mem_cmd_desc)"
}

# /* vim: set filetype=sh : */
